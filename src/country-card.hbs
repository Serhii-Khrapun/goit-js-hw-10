{{#each this}}
<li>
    <img src="{{flags.svg}}" alt="" width="50">
    <span>{{name.official}}</span>
</li>
<li>
    Capital: {{capital}}
</li>
<li>
    Population : {{population}}
</li>
<li class='list-group-item'>Languages:
      {{#each languages}}
        {{#if @last}}
          <span>{{this}}</span>
        {{else}}
          <span>{{this}},</span>
        {{/if}}
      {{/each}}</li>
{{/each}}